<!DOCTYPE html>
<html>
  <head>
    <title>APISnapshot</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_pack_tag 'application' %>
  </head>

  <body>
    <header class='navbar navbar-expand flex-column flex-md-row'>
      <div class="container">
          <a class="navbar-brand" href="#">APISnapshot</a>
          <div class="collapse navbar-collapse" id="navbarNav">
            <ul class="navbar-nav ml-auto">
              <li class="nav-item">
                <a class="nav-link" href="https://github.com/bigbinary/apisnapshot">GitHub</a>
              </li>
            </ul>
          </div>
      </div>
    </header>
    <div class='container main'>
      <%= yield %>
    </div>

    <%= javascript_pack_tag "application" %>
  </body>
</html>
